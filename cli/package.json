{
  "name": "validate-cli",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "./dist/main.js.js",
  "module": "./dist/main.js",
  "bin": {
    "validate": "./bin/validate.js"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "files": [
    "dist",
    "bin",
    "*.d.ts",
    "*.mjs"
  ],
  "scripts": {
    "fmt": "biome format .",
    "fmt:fix": "biome format . --write",
    "lint": "pnpm biome lint ./src",
    "lint:fix": "pnpm biome lint ./src --apply",
    "start": "tsx src/entrypoint.ts",
    "build": "tsup src --format esm --no-splitting --sourcemap ",
    "watch": "tsup src --format esm --no-splitting --watch",
    "test": "vitest run"
  },
  "keywords": [],
  "author": "moonsong-labs",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.6.4",
    "@types/node": "^20.12.6",
    "typescript": "^5.4.4",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "@octokit/core": "^6.1.2",
    "@types/yargs": "^17.0.32",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.4",
    "dotenv": "^16.4.5",
    "ora": "^8.0.1",
    "tempy": "^3.1.0",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "viem": "^2.9.15",
    "yargs": "^17.7.2",
    "zksync-ethers": "^6.7.0",
    "zod": "^3.22.4"
  }
